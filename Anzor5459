    while true do
        wait(math.random(20,30))
        game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
        wait(2)
loadstring(game:HttpGet('https://raw.githubusercontent.com/Anzor5458/Nope-/refs/heads/main/Pressure%202'))()
    end
end)()
